<p id="notice"><%= notice %></p>
<center>
<h1>Games</h1>

<table width="60%">
  <thead>
    <tr>
      <th>Name</th>            
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>                
        <td align="center"><%= link_to game.name, "games/" + game.id.to_s %></td>        
      </tr>
    <% end %>
  </tbody>
</table>
</center>
<br>

